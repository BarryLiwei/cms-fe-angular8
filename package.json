{
    "name": "CMS-FE",
    "version": "1.0.0",
    "description": "angular5 for CMS",
    "main": "index.js",
    "author": "xpioneer",
    "keywords": [
        "angular4",
        "HttpInterceptor",
        "lazyload",
        "ng-zorro-antd",
        "loadChildren"
    ],
    "license": "MIT",
    "scripts": {
        "start": "NODE_ENV=development webpack-dev-server --inline --colors --progress --config conf/webpack.dev.conf.js",
        "dll": "webpack --config conf/webpack.dll.conf.js",
        "clean": "rm -rf dist",
        "build": "NODE_ENV=production yarn run clean && webpack --config conf/webpack.prod.conf.js",
        "test": "NODE_ENV=development yarn run dll && webpack --config conf/webpack.dev.conf.js",
        "lint": "NODE_ENV=development node_modules/.bin/tslint -c tslint.json -p tsconfig.json",
        "lintfix": "NODE_ENV=development node_modules/.bin/tslint -c tslint.json -p tsconfig.json --fix --force"
    },
    "dependencies": {
        "@angular/animations": "^5.0.1",
        "@angular/common": "^5.0.1",
        "@angular/compiler": "^5.0.1",
        "@angular/core": "^5.0.1",
        "@angular/forms": "^5.0.1",
        "@angular/http": "^5.0.1",
        "@angular/platform-browser": "^5.0.1",
        "@angular/platform-browser-dynamic": "^5.0.1",
        "@angular/router": "^5.0.1",
        "core-js": "2.4.1",
        "echarts": "^3.8.5",
        "ng-zorro-antd": "^0.6.0-rc.3",
        "quill": "^1.3.2",
        "reflect-metadata": "^0.1.10",
        "rxjs": "^5.5.2",
        "zone.js": "0.8.12"
    },
    "devDependencies": {
        "@types/node": "^8.0.24",
        "angular2-router-loader": "^0.3.5",
        "angular2-template-loader": "^0.6.2",
        "autoprefixer": "^7.1.4",
