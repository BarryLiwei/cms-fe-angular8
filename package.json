        "clean-webpack-plugin": "^0.1.16",
        "css-loader": "^0.28.5",
        "extract-text-webpack-plugin": "^3.0.0",
        "file-loader": "^0.11.2",
        "html-loader": "^0.5.0",
        "html-webpack-plugin": "^2.30.1",
        "less": "^2.7.2",
        "less-loader": "^4.0.5",
        "postcss-loader": "^2.0.6",
        "raw-loader": "^0.5.1",
        "style-loader": "^0.18.2",
        "tslint": "^5.7.0",
        "tslint-loader": "^3.5.3",
        "typescript": "^2.4.2",
        "uglifyjs-webpack-plugin": "^0.4.6",
